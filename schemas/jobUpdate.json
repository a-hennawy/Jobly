{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Root Schema",
    "required": [],
    "properties": {
        "title": {
            "type": "string",
            "title": "The title Schema",
            "examples": [
                "Junior Software engineer"
            ],
            "minLength": 1,
            "maxLength": 30
        },
        "salary": {
            "type": "integer",
            "title": "The salary Schema",
            "examples": [
                4000
            ],
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.3
            ],
            "maximum": 1.0
        },
        "company_handle": {
            "type": "string",
            "title": "The company_handle Schema",
            "examples": [
                "watson-davis"
            ],
            "minLength": 1,
            "maxLength": 30
        }
    },
    "examples": [{
        "title": "Junior Software engineer",
        "salary": 4000,
        "equity": 0.3,
        "company_handle": "watson-davis"
    }]
}